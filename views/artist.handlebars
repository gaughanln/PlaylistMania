{{#if logged_in}}
{{> full-header}}

<div class="center-align">
  <div class="col s12 white-text genres">
    <h1> party on wayne 🤘🏽 </h1>
  </div>
</div>

<div class="container">
  <div class="playlist-img">
    <img src="images/dj.gif" alt="playlist graphic">
  </div>

  <div class="title-two">
    <h2 class="white-text search-title">search by artist</h2>
    <div class="search-main">
      <input class="search-input white-text" id="search-bar" type="text" placeholder="Search artist" />
      <div class="center-align">
        <a class=" white-text purple accent-4 waves-effect btn-large submit artist-btn">
          <i class=" material-icons left">audiotrack
          </i>Search</a>
      </div>
    </div>
  </div>
</div>

<div id="results" class="row"></div>

<script src="js/index.js"></script>
<script src="js/logout.js"></script>
{{/if}}
